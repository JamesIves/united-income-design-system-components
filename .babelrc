{
  "plugins": [
    "@babel/transform-spread",
    "@babel/transform-runtime",
    ["module-resolver", {
      "alias": {
        "~components": "./components",
        "~proptypes": "./proptypes",
        "~constants": "./constants"
      }
    }]
  ],
  "presets": [
    ["@babel/env", {
      "debug": true,
      "modules": false,
      "useBuiltIns": "usage"
    }],
    "@babel/react"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/env", {
          "modules": "commonjs",
          "useBuiltIns": "usage"
        }],
        "@babel/react"
      ],
      "plugins": [
        "istanbul"
      ]
    },
  }
}
