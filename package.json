{
  "name": "@unitedincome/components",
  "description": "React library for all United Income Front-End projects.",
  "author": "United Income",
  "version": "0.1.4",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "build": "webpack",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280 -s storybook-static",
    "test": "yarn testSingle './components/**/*.spec.js' './proptypes/**/*.spec.js' | faucet",
    "testCI": "NODE_ENV=test nyc tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles './components/**/*.spec.js' './proptypes/**/*.spec.js'  | tap-xunit > ./reports/tape/results.xml",
    "coverage": "nyc report",
    "testVr": "jest vr-tests",
    "testSingle": "NODE_ENV=test tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles",
    "buildStorybook": "build-storybook -c .storybook -o .build",
    "publishStorybook": "yarn buildStorybook && ./publishStorybook.sh",
    "lint": "eslint './stories/**/*.js' './components/**/*.js' './proptypes/**/*.js' './.storybook/*.js' --cache && sass-lint 'component/**/*.sass' -qv && prettier -l './components/**/*.js' './proptypes/**/*.js' && prettier -l './components/**/*.scss' --parser=css || (echo need formatting, use yarn format ; exit 1)",
    "format": "prettier --write './components/**/*.js' './proptypes/**/*.js' './stories/**/*.js' './.storybook/*.js' && prettier --write  './components/**/*.md' './stories/**/*.md' './*.md' --parser=markdown && prettier --write './components/**/*.scss' --parser=css"
  },
  "homepage": "https://github.com/UnitedIncome/components",
  "repository": {
    "type": "git",
    "url": "https://github.com/UnitedIncome/components.git"
  },
  "bugs": {
    "url": "https://github.com/UnitedIncome/components/issues"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-transform-runtime": "*",
    "@babel/plugin-transform-spread": "*",
    "@babel/polyfill": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/register": "*",
    "@babel/runtime": "*",
    "@percy-io/percy-storybook": "*",
    "@sambego/storybook-state": "*",
    "@storybook/addon-a11y": "*",
    "@storybook/addon-backgrounds": "*",
    "@storybook/addon-knobs": "*",
    "@storybook/addon-viewport": "*",
    "@storybook/react": "*",
    "airbnb-prop-types": "*",
    "babel-loader": "*",
    "babel-plugin-istanbul": "*",
    "bootstrap": "*",
    "classnames": "*",
    "css-loader": "*",
    "d3-interpolate": "*",
    "d3-scale": "*",
    "d3-shape": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "faucet": "*",
    "file-loader": "*",
    "ignore-styles": "*",
    "jsdom": "*",
    "jsdom-global": "*",
    "mini-css-extract-plugin": "*",
    "node-sass": "*",
    "nyc": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss-loader": "*",
    "postcss-prefixer": "*",
    "prettier": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-text-mask": "*",
    "sass-lint": "*",
    "sass-loader": "*",
    "sinon": "*",
    "storybook-readme": "*",
    "style-loader": "*",
    "tap-xunit": "*",
    "tape": "*",
    "text-mask-addons": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "*"
  },
  "peerDependencies": {
    "classnames": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-text-mask": "*"
  }
}
