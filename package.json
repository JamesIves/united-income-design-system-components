{
  "name": "@unitedincome/components",
  "description": "React library for all United Income Front-End projects.",
  "author": "United Income",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "build": "webpack",
    "test": "yarn testSingle 'tests/**/*.js' | faucet",
    "testCI": "NODE_ENV=test nyc tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles 'tests/**/*.js'  | tap-xunit > ./reports/tape/results.xml",
    "coverage": "nyc report",
    "testVr": "jest vr-tests",
    "testSingle": "NODE_ENV=test tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles",
    "buildStorybook": "build-storybook -c .storybook -o .build",
    "publishStorybook": "yarn buildStorybook && ./publishStorybook.sh",
    "lint": "eslint './stories/**/*.js' './components/**/*.js' --cache && sass-lint 'component/**/*.sass' -qv && prettier -l './components/**/*.js' && prettier -l './components/**/*.scss' --parser=css || (echo need formatting, use yarn format ; exit 1)",
    "format": "prettier --write './components/**/*.js' './stories/**/*.js' && prettier --write './components/**/*.scss' --parser=css"
  },
  "homepage": "https://github.com/UnitedIncome/component",
  "repository": {
    "type": "git",
    "url": "https://github.com/UnitedIncome/component.git"
  },
  "bugs": {
    "url": "https://github.com/UnitedIncome/component/issues"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-transform-runtime": "*",
    "@babel/plugin-transform-spread": "*",
    "@babel/polyfill": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/register": "*",
    "@babel/runtime": "*",
    "@storybook/addon-knobs": "*",
    "@storybook/react": "*",
    "airbnb-prop-types": "*",
    "babel-loader": "*",
    "babel-plugin-istanbul": "*",
    "classnames": "*",
    "css-loader": "*",
    "d3-scale": "*",
    "d3-shape": "*",
    "d3-interpolate": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "faucet": "*",
    "ignore-styles": "*",
    "jest": "*",
    "jsdom": "*",
    "jsdom-global": "*",
    "mini-css-extract-plugin": "*",
    "node-sass": "*",
    "nyc": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "prettier": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-text-mask": "*",
    "sass-lint": "*",
    "sass-loader": "*",
    "sinon": "*",
    "style-loader": "*",
    "text-mask-addons": "*",
    "tap-xunit": "*",
    "tape": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "*"
  },
  "peerDependencies": {
    "classnames": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "react-text-mask": "*"
  }
}
